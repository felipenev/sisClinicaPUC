<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <converter>
  <converter-for-class>java.lang.Enum</converter-for-class>
  <converter-class>javax.faces.convert.EnumConverter</converter-class>
 </converter>
 <!-- Geral -->
 
<navigation-rule>
  <from-view-id>/*</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/pages/principal.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 
 <navigation-rule>
  <from-view-id>/pages/manter_objetivo_estrategico/objetivoEstrategicoPesquisa.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>cadastro</from-outcome>
   <to-view-id>/pages/manter_objetivo_estrategico/objetivoEstrategicoCadastro.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <resource-bundle>
   <base-name>br.com.sisClinicaPUC.mensagens.messages</base-name>
   <var>msgs</var>
  </resource-bundle>
  <locale-config>
   <default-locale>pt_BR</default-locale>
   <supported-locale>pt_BR</supported-locale>
   <supported-locale>en_US</supported-locale>
  </locale-config>
 </application>
</faces-config>
